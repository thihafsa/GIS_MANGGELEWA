        <div class="content-body">
            <div class="container-fluid">
                <div class="row invoice-card-row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="form-validation">
                                    <div class="row">
                                        <div class="col-xl-6">
                                            <div class="mb-3">
                                                <label class="form-label" for="nama_fasilitas">Nama Fasilitas <span
                                                        class="text-danger">*</span></label>
                                                <input type="text" class="form-control" id="nama_fasilitas"
                                                    name="nama_fasilitas" placeholder="Masukkan nama fasilitas.."
                                                    required>
                                                <div class="invalid-feedback">
                                                    Harap masukkan nama fasilitas.
                                                </div>
                                            </div>
                                            <input type="hidden" id="idjenis" name="idjenis" value="<%=idJenisfasil%>">

                                            <div class="mb-3">
                                                <label class="form-label" for="jam_buka">Jam Buka <span
                                                        class="text-danger">*</span></label>
                                                <div class="input-group clockpicker" data-placement="bottom"
                                                    data-align="top" data-autoclose="true">
                                                    <input type="text" class="form-control" id="jam_buka"
                                                        name="jam_buka" value="09:30">
                                                    <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
                                                </div>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="jam_tutup">Jam Tutup <span
                                                        class="text-danger">*</span></label>
                                                <div class="input-group clockpicker" data-placement="bottom"
                                                    data-align="top" data-autoclose="true">
                                                    <input type="text" class="form-control" id="jam_tutup"
                                                        name="jam_tutup" value="17:30">
                                                    <span class="input-group-text"><i class="fa fa-clock-o"></i></span>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-xl-6">
                                            <div class="mb-3">
                                                <label class="form-label" for="latitude">Latitude</label>
                                                <input type="number" class="form-control" id="latitude" name="latitude"
                                                    step="0.000001" placeholder="Masukkan latitude..">
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="longitude">Longitude</label>
                                                <input type="number" class="form-control" id="longitude"
                                                    name="longitude" step="0.000001" placeholder="Masukkan longitude..">
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="alamat">Alamat <span
                                                        class="text-danger">*</span></label>
                                                <div class="input-group">
                                                    <input type="text" class="form-control" id="alamat" name="alamat"
                                                        placeholder="Masukkan alamat.." required>
                                                    <button type="button" class="btn btn-primary"
                                                        onclick="generateAddress()">Generate</button>
                                                </div>
                                                <div class="invalid-feedback">Harap masukkan alamat.</div>
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="fasilitas">Fasilitas</label>
                                                <select class="multi-select" id="fasilitas" name="fasilitas[]"
                                                    multiple="multiple">
                                                    <% listFasilitas.forEach(fasilitas => { %>
                                                    <option value="<%= fasilitas %>"><%= fasilitas %></option>
                                                    <% }); %>
                                                </select>
                                            </div>
                                        </div>


                                        <div class="col-xl-6">
                                            <div class="mb-3">
                                                <label class="form-label" for="foto">Foto</label>
                                                <input type="file" class="form-control" id="foto" name="foto"
                                                    onchange="previewImage(this);">
                                                <img id="imagePreview" src="#" alt="Image Preview"
                                                    style="display: none; max-width: 200px; max-height: 200px; margin-top: 10px;">
                                            </div>
                                            <div class="mb-3">
                                                <label class="form-label" for="deskripsi">Deskripsi</label>
                                                <textarea class="form-control" id="deskripsi" name="deskripsi" rows="5"
                                                    placeholder="Deskripsikan fasilitas.." required></textarea>
                                            </div>


                                        </div>

                                        <div class="col-12">
                                            <div class="mb-3">
                                                <button type="submit" class="btn btn-primary"
