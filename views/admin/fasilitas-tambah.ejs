        <script>
            function generateDescription() {
                const nama_instansi = document.getElementById('nama_fasilitas').value;
                const fasilitas = Array.from(document.getElementById('fasilitas').selectedOptions).map(opt =>
                    opt.value).join(',');
                const alamat = document.getElementById('alamat').value;
                const jam_buka = document.getElementById('jam_buka').value;
                const jam_tutup = document.getElementById('jam_tutup').value;
                console.log(nama_instansi);
                fetch('/ai/generate-description', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                        },
                        body: JSON.stringify({
                            nama_instansi,
                            kepala,
                            fasilitas,
                            alamat,
                            jam_buka,
                            jam_tutup
                        }),
                    })
                    .then(response => {
                        if (!response.ok) {
                            throw new Error('Network response was not ok');
                        }
                        return response.json();
                    })
                    .then(data => {
                        document.getElementById('deskripsi_singkat').value = ''
                        document.getElementById('deskripsi_singkat').value = data.description;
                    })
                    .catch(error => {
                        console.error('Error:', error);
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Gagal menghasilkan deskripsi singkat. Silakan coba lagi.'
                        });
                    });
            }

            function generateAddress() {
                const lat = document.getElementById('latitude').value;
                const lng = document.getElementById('longitude').value;

                // Validasi apakah latitude atau longitude kosong
                if (!lat || !lng) {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'Latitude dan Longitude harus diisi untuk menghasilkan alamat.'
                    });
                    return;
                }

                const apiKey = 'AIzaSyD4P3rMVrMzwD_8l1I1ETldunN6Vdx7Tqk'; // Ganti dengan API Key Anda

                fetch(`https://maps.googleapis.com/maps/api/geocode/json?latlng=${lat},${lng}&key=${apiKey}`)
                    .then(response => {
                        if (!response.ok) {
                            throw new Error('Failed to fetch address');
                        }
                        return response.json();
                    })
                    .then(data => {
                        if (data.results.length > 0) {
                            const addressComponents = data.results[0].address_components;
                            // Cari dan gabungkan komponen alamat kecuali kode lokasi (G74X+F8V)
                            const filteredAddress = addressComponents
                                .filter(component => !component.types.includes('plus_code'))
                                .map(component => component.long_name)
                                .join(', ');

                            document.getElementById('alamat').value = filteredAddress;
                        } else {
                            document.getElementById('alamat').value = 'Alamat tidak ditemukan';
                        }
                    })
                    .catch(error => {
                        console.error('Error fetching address:', error);
                        document.getElementById('alamat').value = 'Error: Gagal mengambil alamat';
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Gagal mengambil alamat dari Google Maps. Silakan coba lagi.'
                        });
                    });
            }


           async function submitForm() {
                const formData = new FormData();
                formData.append('nama_fasilitas', document.getElementById('nama_fasilitas').value);
                formData.append('jam_buka', document.getElementById('jam_buka').value);
                formData.append('jam_tutup', document.getElementById('jam_tutup').value);
                formData.append('alamat', document.getElementById('alamat').value);
                formData.append('latitude', document.getElementById('latitude').value);
                formData.append('longitude', document.getElementById('longitude').value);
                formData.append('deskripsi', document.getElementById('deskripsi').value);

                // Mengambil fasilitas yang dipilih
                const fasilitas = Array.from(document.getElementById('fasilitas').selectedOptions).map(opt =>
                    opt.value).join(',');
                formData.append('fasilitas', fasilitas);

                // Menambahkan idjenis jika diperlukan
                const idjenis = document.getElementById('idjenis').value;
                formData.append('idjenis', idjenis);

                // Menambahkan foto jika ada
                const foto = document.getElementById('foto').files[0];
                if (foto) {
                    formData.append('foto', foto);
                }

                try {
                    const response = await fetch('/fasilitas', {
                        method: 'POST',
                        body: formData
                    });

                    const data = await response.json();
                    if (response.ok) {
                        // Handle success response
                        sessionStorage.setItem('submitSuccess', 'Data berhasil ditambah!');
                        // Redirect to admin page (sesuaikan URL ini jika diperlukan)
                        window.location.href = '/admin/<%=jenisFasilitas%>';
                    } else {
                        // Login gagal
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: data.message,
                        });
                    }
                } catch (error) {
                    // Handle error
                    console.error('Error:', error);
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: error.message // Pesan kesalahan yang diambil dari server
                    });
                }
            }


            function previewImage(input) {
                const file = input.files[0];
                const reader = new FileReader();

                reader.onload = function (e) {
                    const preview = document.getElementById('imagePreview');
                    preview.src = e.target.result;
                    preview.style.display = 'block';
                };

                reader.readAsDataURL(file);
            }
        </script>