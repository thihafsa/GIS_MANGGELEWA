    <script>
        // Ambil pesan sukses dari session storage
        const submitSuccessMessage = sessionStorage.getItem('submitSuccess');
        if (submitSuccessMessage) {
            // Tampilkan alert menggunakan SweetAlert2
            Swal.fire({
                icon: 'success',
                title: 'Success',
                text: submitSuccessMessage,
                timer: 1500
            }).then(() => {
                // Hapus pesan dari session storage setelah ditampilkan
                sessionStorage.removeItem('submitSuccess');
            });
        }
    </script>
